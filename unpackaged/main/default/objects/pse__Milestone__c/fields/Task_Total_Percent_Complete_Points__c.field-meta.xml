<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Task_Total_Percent_Complete_Points__c</fullName>
    <formula>IF ( pse__Total_Points_from_Tasks__c =0, 0,  pse__Total_Completed_Points_from_Tasks__c/pse__Total_Points_from_Tasks__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Task percent complete based on Task Points</inlineHelpText>
    <label>Task Total Percent Complete (Points)</label>
    <precision>18</precision>
    <required>false</required>
