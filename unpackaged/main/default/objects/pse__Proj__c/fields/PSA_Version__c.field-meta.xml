<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PSA_Version__c</fullName>
    <description>To indicate if this Project was created in version PSA1.0 or PSA2.0</description>
    <externalId>false</externalId>
    <formula>IF( CreatedDate &lt; DATETIMEVALUE( DATE(2021, 01, 18)), "PSA 1.0", "PSA 2.0")</formula>
    <label>PSA Version</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
