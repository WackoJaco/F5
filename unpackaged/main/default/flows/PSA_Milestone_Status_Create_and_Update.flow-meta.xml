    <decisions>
        <name>Check_for_Expiration_date</name>
        <label>Check for Expiration date</label>
        <locationX>1238</locationX>
        <locationY>395</locationY>
        <defaultConnector>
            <targetReference>Assign_Check_Closed_for_Expense_Entry_Time_Entry_for_Not_expired_records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Expiration_Date__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
